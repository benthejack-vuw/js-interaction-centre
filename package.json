  "repository": {
    "type": "git",
    "url": "https://github.com/benthejack-vuw/js-interaction-centre"
  },
  "scripts": {
    "prepublish": "tsc && babel lib -d lib && webpack"
  },
  "keywords": [
    "interaction"
  ],
  "author": "Ben Jack",
  "license": "ISC",
  "dependencies": {
    "bj-utils": "^1.0.7",
    "keyboardevent-key-polyfill": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  }
}
